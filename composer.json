{
    "name": "yiisoft/yii2-app-advanced",
    "description": "Yii 2 Advanced Application Template",
    "keywords": ["yii2", "framework", "advanced", "application template"],
    "homepage": "http://www.yiiframework.com/",
    "type": "project",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/yiisoft/yii2/issues?state=open",
        "forum": "http://www.yiiframework.com/forum/",
        "wiki": "http://www.yiiframework.com/wiki/",
        "irc": "irc://irc.freenode.net/yii",
        "source": "https://github.com/yiisoft/yii2"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": "2.0.8",
        "yiisoft/yii2-bootstrap": "2.0.6",
        "yiisoft/yii2-swiftmailer": "2.0.5",
        "kartik-v/yii2-widgets": "v3.4.0",
        "kartik-v/yii2-sortable-input": "v1.1.0",
        "2amigos/yii2-ckeditor-widget": "1.0.4",
        "iutbay/yii2-fontawesome": "0.0.1",
        "yiisoft/yii2-imagine": "^2.0@dev",
        "pavlinter/yii2-display-image": "1.0.0",
        "kartik-v/yii2-widget-fileinput": "v1.0.4",
        "kartik-v/yii2-datecontrol": "1.9.4",
        "iutbay/yii2-kcfinder": "0.0.1",
        "sersid/yii2-owl-carousel-asset": "dev-master",
        "raoul2000/yii2-scrollup-widget": "1.1.0",
        "creocoder/yii2-taggable": "dev-master",
        "xj/yii2-jplayer-widget": "2.7.0.3",
        "fruppel/yii2-googlecharts" : "v1.1.0",
        "zelenin/yii2-rss": "dev-master",
        "yiisoft/yii2-jui": "^2.0.5",
        "2amigos/yii2-google-maps-library": "1.0.0",
        "kartik-v/yii2-mpdf": "dev-master",
        "cinghie/yii2-cookie-consent": "v1.4.3"
    },
    "require-dev": {
        "yiisoft/yii2-codeception": "2.0.5",
        "yiisoft/yii2-debug": "2.0.6",
        "yiisoft/yii2-gii": "^2.0.5",
        "yiisoft/yii2-faker": "2.0.3"
    },
    "config": {
        "vendor-dir": "private/vendor",
        "process-timeout": 1800
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "private/vendor/npm",
            "bower-asset-library": "private/vendor/bower"
        }
    }
}
